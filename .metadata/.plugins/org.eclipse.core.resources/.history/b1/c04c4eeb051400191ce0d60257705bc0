import com.google.gson.annotations.Expose;
import com.google.gson.annotations.SerializedName;

public class Players {

@SerializedName("ID")
@Expose
private Integer iD;
@SerializedName("Voornaam")
@Expose
private String voornaam;
@SerializedName("Achternaam")
@Expose
private String achternaam;
@SerializedName("Usernaam")
@Expose
private String usernaam;
@SerializedName("Wachtwoord")
@Expose
private String wachtwoord;
@SerializedName("Beheerder")
@Expose
private Boolean beheerder;
@SerializedName("Tussenvoegsels")
@Expose
private String tussenvoegsels;



/**
* No args constructor for use in serialization
* 
*/
public Players() {
}

/**
* 
* @param beheerder
* @param wachtwoord
* @param achternaam
* @param tussenvoegsels
* @param voornaam
* @param usernaam
* @param iD
*/
public Players(Integer iD, String voornaam, String tussenvoegsels, String achternaam, String usernaam, String wachtwoord, Boolean beheerder) {
super();
this.iD = iD;
this.voornaam = voornaam;
this.tussenvoegsels = tussenvoegsels;
this.achternaam = achternaam;
this.usernaam = usernaam;
this.wachtwoord = wachtwoord;
this.beheerder = beheerder;
}

public Integer getID() {
return iD;
}

public void setID(Integer iD) {
this.iD = iD;
}

public String getVoornaam() {
return voornaam;
}

public void setVoornaam(String voornaam) {
this.voornaam = voornaam;
}

public String getTussenvoegsels() {
	return tussenvoegsels;
}

public void setTussenvoegsels(String tussenvoegsels) {
	this.tussenvoegsels = tussenvoegsels;
}

public String getAchternaam() {
return achternaam;
}

public void setAchternaam(String achternaam) {
this.achternaam = achternaam;
}

public String getUsernaam() {
return usernaam;
}

public void setUsernaam(String usernaam) {
this.usernaam = usernaam;
}

public String getWachtwoord() {
return wachtwoord;
}

public void setWachtwoord(String wachtwoord) {
this.wachtwoord = wachtwoord;
}

public Boolean getBeheerder() {
return beheerder;
}

public void setBeheerder(Boolean beheerder) {
this.beheerder = beheerder;
}


}